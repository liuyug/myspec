%define __os_install_post /usr/lib/rpm/brp-compress %{nil}
Name:		freemind
Version:	1.0.0
Release:	1%{?dist}
Summary:	FreeMind is a premier free mind-mapping software written in Java.

Group:		Applications/Editors
License:	GPLv2+
URL:		http://freemind.sourceforge.net/wiki/index.php/Main_Page
Source0:	%{name}-bin-max-%{version}.zip
BuildRoot:	%(mktemp -ud %{_tmppath}/%{name}-%{version}-%{release}-XXXXXX)


%description
FreeMind is a premier free mind-mapping software written in Java.


%prep
%setup -q -c -n %{name}-%{version}

%install

rm -rf %{buildroot}

mkdir -p %{buildroot}/opt/freemind
cp -r . %{buildroot}/opt/freemind

# make wraper for freemind
mkdir -p %{buildroot}/usr/bin
cat <<EOF > %{buildroot}/usr/bin/freemind
#!/bin/sh
export FREEMIND_BASE_DIR="/opt/freemind"
\${FREEMIND_BASE_DIR}/freemind.sh \${1+"\$@"}
EOF
chmod 0755 %{buildroot}/usr/bin/freemind %{buildroot}/opt/freemind/freemind.sh

# make desktop, pixmaps, mime
mkdir -p %{buildroot}/usr/share/{applications,pixmaps,mime/packages}

cat <<EOF > %{buildroot}/usr/share/applications/freemind.desktop
[Desktop Entry]
Version=1.0
Exec=freemind
TryExec=freemind
Icon=freemind
Terminal=false
Name=FreeMind
GenericName=FreeMind
Comment=Mind Mapping Tool
Categories=Office;
Type=Application
MimeType=application/x-freemind;
StartupNotify=true
EOF
cat <<EOF > %{buildroot}/usr/share/mime/packages/freemind.xml
<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
  <mime-type type="application/x-freemind">
    <comment>FreeMind Map</comment>
    <magic priority="80">
      <match value="&lt;map version" type="string" offset="0:20"/>
         <match value="FreeMind" type="string" offset="50:100"/>
        </magic>
        <glob pattern="*.mm"/>
    </mime-type>
</mime-info>
EOF
